class Student {
    String name;
    int age;

    void display() {
        System.out.println("Name: " + name + ", Age: " + age);
    }
}

public class Main1 {
    public static void main(String[] args) {
        Student s1 = new Student();
        s1.name = "Charul Bhanarkar";
        s1.age = 18;
        s1.display();
    }
}
